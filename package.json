{
  "name": "devkeeper",
  "label": "devkeeper",
  "version": "0.1.23",
  "description": "Keeps node module development environment up to date. Installs config files, development dependencies and modifies your package.json file and keeps them updated.",
  "keywords": [
    "scripts",
    "boilerplate",
    "keep",
    "development keeper"
  ],
  "engines": {
    "node": ">=10.8.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/bin",
    "/lib/**/!(*.spec|*.test)*.*",
    "/dist/**/!(*.spec|*.test)*.*",
    "/@types",
    "/module-files",
    "LICENSE",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "bin": {
    "devkeeper": "./bin/run"
  },
  "homepage": "https://github.com/ozum/devkeeper",
  "bugs": "https://github.com/ozum/devkeeper/issues",
  "repository": "github:ozum/devkeeper",
  "author": {
    "name": "Özüm Eldoğan"
  },
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc -b",
    "commit": "git-cz",
    "commit:add": "git add -A && npm run commit",
    "devkeeper:update": "devkeeper update",
    "docs:build": "npm-run-all -p typedoc:md typedoc:html && rm -rf docs/nav.02.api docs/.vuepress/public/api-site && mv api-docs-md docs/nav.02.api && mv api-docs-html docs/.vuepress/public/api-site && cp assets/typedoc/01.typedoc-iframe.md docs/nav.02.api/ && vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "format": "prettier --write --config .prettierrc --check 'src/**/*.+(js|jsx|json|less|css|ts|tsx|md|vue|gql)'",
    "lint": "eslint --max-warnings=0 --ignore-path .gitignore --cache 'src/**/*.+(js|jsx|ts|tsx|vue)'",
    "postinstall": "bin/run update",
    "postpack": "rm -f oclif.manifest.json",
    "postreadme": "oclif-dev readme",
    "prepack": "oclif-dev manifest",
    "readme": "if grep -q '{% include \"api.md\" %}' 'README.njk'; then npm run typedoc:single-md; mkdir -p temp && mv api.md temp/; fi && readmeasy --partial-dirs temp && rm -rf temp",
    "release": "(git diff --quiet && git diff --staged --quiet && echo No files has been changed) || (npm-run-all -p build commit:add && git checkout master && git pull origin master && standard-version --no-verify && npm run readme && git add README.md && git commit --amend --no-edit && git push --follow-tags origin master && git push --tags && npm publish)",
    "test": "NODE_ENV=test jest --coverage --config=jest.config.json",
    "test:watch": "NODE_ENV=test jest --watch --config=jest.config.json",
    "typedoc:html": "rm -rf api-docs-html && typedoc --plugin typedoc-plugin-example-tag,typedoc-neo-theme --theme ./node_modules/typedoc-neo-theme/bin/default --mode file --out api-docs-html",
    "typedoc:md": "rm -rf api-docs-md && typedoc --platform vuepress --plugin typedoc-plugin-example-tag,typedoc-plugin-markdown --excludeExternals --excludePrivate --excludeProtected --theme markdown --readme none --mode file --out api-docs-md && find api-docs-md -name \"index.md\" -exec sh -c 'mv \"$1\" \"${1%index.md}\"index2.md' - {} \\;",
    "typedoc:single-md": "npm run typedoc:md && concat-md --dir-name-as-title api-docs-md > api.md && rm -rf api-docs-md",
    "update": "npm-run-all build update-packages && ./bin/run update",
    "update-packages": "find module-files/package-json -name \"package.json\" -exec ncu -u --packageFile {} \\; && npm run postinstall && npm install",
    "validate": "npm-run-all -p lint test"
  },
  "shields": [
    "commitizen",
    "conventionalcommits"
  ],
  "dependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.1",
    "@types/prettier": "^1.18.3",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "@vuepress/plugin-active-header-links": "^1.2.0",
    "@vuepress/plugin-pwa": "^1.2.0",
    "commitizen": "^4.0.3",
    "concat-md": "^0.3.2",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-plugin-prettier": "^3.1.1",
    "globby": "^10.0.1",
    "husky": "^3.0.9",
    "intermodular": "^0.5.11",
    "jest": "^24.9.0",
    "jsonlint": "^1.6.3",
    "licensor": "^4.3.0",
    "lint-staged": "^9.4.2",
    "lodash.differencewith": "^4.5.0",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lodash.pick": "^4.4.0",
    "lodash.uniqwith": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.0",
    "prettier": "^1.18.2",
    "readmeasy": "^0.1.4",
    "resolve-cwd": "^3.0.0",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.1.0",
    "ts-node-dev": "^1.0.0-pre.43",
    "typedoc": "github:TypeStrong/typedoc",
    "typedoc-neo-theme": "^1.0.2",
    "typedoc-plugin-example-tag": "^1.0.2",
    "typedoc-plugin-external-module-map": "^1.0.0",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-markdown": "^2.2.11",
    "typedoc-plugin-npm-externals": "^1.0.0",
    "typescript": "^3.6.4",
    "vuepress": "^1.2.0",
    "vuepress-bar": "^0.1.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/lodash.differencewith": "^4.5.6",
    "@types/lodash.escaperegexp": "^4.1.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/lodash.uniqwith": "^4.5.6",
    "@types/nunjucks": "^3.1.2",
    "cli-ux": "^5.3.2",
    "npm-check-updates": "^3.1.24"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "devkeeper",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "vuepress-featureModifications": {
    "files": [
      ".gitignore",
      ".commitlintrc",
      ".czrc",
      ".editorconfig",
      ".eslintignore",
      ".eslintrc.js",
      ".gitattributes",
      ".gitignore-to-rename",
      ".huskyrc",
      ".lintstagedrc",
      ".prettierrc",
      "jest.config.json",
      "tsconfig.json"
    ],
    "objectKeys": [
      "name"
    ],
    "arrayKeys": {
      "files": []
    },
    "version": "0.1.16"
  },
  "common-featureModifications": {
    "files": [
      ".gitignore",
      ".commitlintrc",
      ".czrc",
      ".editorconfig",
      ".eslintignore",
      ".eslintrc.js",
      ".gitattributes",
      ".gitignore-to-rename",
      ".huskyrc",
      ".lintstagedrc",
      ".prettierrc",
      "jest.config.json",
      "tsconfig.json"
    ],
    "objectKeys": [
      "dependencies.@commitlint/cli",
      "dependencies.@commitlint/config-conventional",
      "dependencies.@types/jest",
      "dependencies.@types/node",
      "dependencies.@types/prettier",
      "dependencies.@typescript-eslint/eslint-plugin",
      "dependencies.@typescript-eslint/parser",
      "dependencies.commitizen",
      "dependencies.concat-md",
      "dependencies.cz-conventional-changelog",
      "dependencies.eslint",
      "dependencies.eslint-config-airbnb-base",
      "dependencies.eslint-config-prettier",
      "dependencies.eslint-plugin-import",
      "dependencies.eslint-plugin-jest",
      "dependencies.eslint-plugin-prettier",
      "dependencies.husky",
      "dependencies.jest",
      "dependencies.jsonlint",
      "dependencies.licensor",
      "dependencies.lint-staged",
      "dependencies.npm-run-all",
      "dependencies.prettier",
      "dependencies.readmeasy",
      "dependencies.standard-version",
      "dependencies.ts-jest",
      "dependencies.ts-node-dev",
      "dependencies.typedoc",
      "dependencies.typedoc-neo-theme",
      "dependencies.typedoc-plugin-example-tag",
      "dependencies.typedoc-plugin-external-module-map",
      "dependencies.typedoc-plugin-external-module-name",
      "dependencies.typedoc-plugin-markdown",
      "dependencies.typedoc-plugin-npm-externals",
      "dependencies.typescript"
    ],
    "arrayKeys": {
      "files": []
    },
    "version": "0.1.16"
  },
  "devkeeperModifications": {
    "files": [
      ".gitignore",
      ".commitlintrc",
      ".czrc",
      ".editorconfig",
      ".eslintignore",
      ".eslintrc.js",
      ".gitattributes",
      ".gitignore-to-rename",
      ".huskyrc",
      ".lintstagedrc",
      ".prettierrc",
      "jest.config.json",
      "tsconfig.json",
      "assets/typedoc/01.typedoc-iframe.md",
      "docs/.vuepress/config.js"
    ],
    "objectKeys": [
      "scripts.build",
      "scripts.typedoc:md",
      "scripts.typedoc:html",
      "scripts.typedoc:single-md",
      "scripts.readme",
      "scripts.format",
      "scripts.lint",
      "scripts.test",
      "scripts.test:watch",
      "scripts.validate",
      "scripts.commit",
      "scripts.commit:add",
      "scripts.release",
      "scripts.docs:dev",
      "scripts.docs:build",
      "scripts.devkeeper:update",
      "dependencies.readmeasy",
      "dependencies.@vuepress/plugin-active-header-links",
      "dependencies.@vuepress/plugin-pwa",
      "dependencies.vuepress",
      "dependencies.vuepress-bar"
    ],
    "arrayKeys": {
      "files": [
        "/bin",
        "/lib/**/!(*.spec|*.test)*.*",
        "/dist/**/!(*.spec|*.test)*.*",
        "/@types",
        "/module-files",
        "LICENSE"
      ]
    },
    "version": "0.1.20"
  }
}
